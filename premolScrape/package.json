    "@types/node": "^20.8.9",
    "csv-parse": "^5.5.5",
    "csv-stringify": "^6.5.0",
    "electron-log": "^5.1.0",
    "fs": "^0.0.1-security",
    "iconv-lite": "^0.6.3",
    "path": "^0.12.7",
    "puppeteer-core": "^22.9.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "electron": "^29.1.1",
    "electron-packager": "^17.1.2"
