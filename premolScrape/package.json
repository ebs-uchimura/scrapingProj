{
  "name": "premolscrape",
  "productName": "premolscrape",
  "version": "1.0.0",
  "description": "Electron application for scraping",
  "main": "build/premol.js",
  "scripts": {
    "build": "tsc && chcp 65001",
    "package-win": "tsc && chcp 65001 && electron-packager . --asar --overwrite --platform=win32 --arch=ia32 --prune --icon=./assets/beer.ico --out=release-builds",
    "start": "tsc && chcp 65001 && electron ."
  },
